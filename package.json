{
  "name": "express-mongoose-jest",
  "version": "1.0.0",
  "private": true,
  "description": "Minimal API RESTful with MongoDB and tested with Jest",
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "main": "dist/app.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "source ./.env ; tsnd -r tsconfig-paths/register src/app.ts",
    "fixtures:reseed": "seedgoose reseed",
    "fixtures:seed": "seedgoose seed",
    "fixtures:unseed": "seedgoose unseed",
    "lint": "eslint .",
    "postbuild": "tscpaths -p tsconfig.build.json -s src -o dist",
    "prepare": "husky install",
    "start": "node .",
    "test": "jest",
    "test:e2e": "artillery run -q -e functional performance-test.yml",
    "test:load": "artillery run -q -e load -o report-load.json performance-test.yml",
    "test:stress": "artillery run -q -e stress -o report-stress.json performance-test.yml",
    "type-check": "tsc --noEmit --noErrorTruncation --pretty false --incremental false"
  },
  "keywords": [],
  "optionalDependencies": {
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-paginate": "^1.0.2",
    "express-pino-logger": "^7.0.0",
    "mongoose": "^8.9.6",
    "type-fest": "^4.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@shelf/jest-mongodb": "^4.1.4",
    "@types/artillery": "^1.6.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.0",
    "@types/express-paginate": "^1.0.1",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^29.2.4",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "artillery": "^2.0.3",
    "artillery-plugin-expect": "^2.1.0",
    "artillery-plugin-faker": "^5.0.0",
    "artillery-plugin-metrics-by-endpoint": "^1.0.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-no-secrets": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.25.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^15.0.1",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.8.1",
    "prettier-plugin-package": "^1.3.0",
    "prettier-plugin-sorted": "^2.0.0",
    "seedgoose": "^2.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4"
  }
}
